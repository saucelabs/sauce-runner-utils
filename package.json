{
  "name": "sauce-testrunner-utils",
  "version": "2.0.0",
  "description": "Sauce Labs test runner helper libary.",
  "author": "devx <dev@saucelabs.com>",
  "license": "MIT",
  "main": "./lib/index",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "lint": "prettier --check '**/*.{js,ts,mjs,cjs}' && eslint tests/ src/",
    "fmt": "prettier --write '**/*.{js,ts,mjs,cjs}'",
    "test": "jest --env node",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major"
  },
  "keywords": [],
  "engines": {
    "node": ">=16.13.2"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "npm": "^10.2.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/chai": "^4.3.7",
    "@types/jest": "^29.5.5",
    "@types/jquery": "^3.5.22",
    "@types/lodash": "^4.14.199",
    "@types/mock-fs": "^4.13.2",
    "@types/node": "^20.8.4",
    "@types/npm": "^7.19.1",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "mock-fs": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "release-it": "^16.2.1",
    "rimraf": "^5.0.5",
    "spawk": "^1.8.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "files": [
    "lib",
    "LICENSE"
  ]
}
